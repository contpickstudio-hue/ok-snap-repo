{
  "version": 2,
  "public": true,
  "git": {
    "deploymentEnabled": {
      "main": false,
      "site": true
    }
  },
  "rewrites": [
    {
      "source": "/blogs/:slug",
      "destination": "/blogs/:slug.html"
    },
    {
      "source": "/account-delete",
      "destination": "/account-delete.html"
    }
  ],
  "headers": [
    {
      "source": "/recipes.json",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/blogs/:slug.html",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, HEAD, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Accept"
        }
      ]
    },
    {
      "source": "/blogs/:slug",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, HEAD, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Accept"
        }
      ]
    }
  ]
}

